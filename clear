[32m+[m[32mconst inputs = document.querySelectorAll(".input");[m
[32m+[m
[32m+[m
[32m+[m[32mfunction addcl(){[m
[32m+[m	[32mlet parent = this.parentNode.parentNode;[m
[32m+[m	[32mparent.classList.add("focus");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction remcl(){[m
[32m+[m	[32mlet parent = this.parentNode.parentNode;[m
[32m+[m	[32mif(this.value == ""){[m
[32m+[m		[32mparent.classList.remove("focus");[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32minputs.forEach(input => {[m
[32m+[m	[32minput.addEventListener("focus", addcl);[m
[32m+[m	[32minput.addEventListener("blur", remcl);[m
[32m+[m[32m});[m
